{
    "name": "jascha030/wp-auto-semver",
    "require": {
        "symfony/console": "^5.3",
        "symfony/process": "^5.3",
        "symfony/finder": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "composer/composer": "^2.1",
        "composer/installers": "^1.11",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jascha030\\WpAutoSemver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jascha030\\WpAutoSemver\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jascha030",
            "email": "contact@jaschavanaalst.nl"
        }
    ],
	"bin": [
		"bin/wp-ver"
	],
    "minimum-stability": "stable",
    "scripts": {
        "phpunit": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php"
    }
}
